{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/:path*"
      },
      {
        "source": "/admin/:path*",
        "destination": "/admin/:path*"
      },
      {
        "source": "/root/:path*",
        "destination": "/root/:path*"
      },
      {
        "source": "/:path*",
        "destination": "/index"
      }
    ],
    "cleanUrls": true
  }